# Google API key (if you plan to use Gemini)
GOOGLE_API_KEY="your_google_api_key_here"

# AWS Bedrock configuration
AWS_BEDROCK_MODEL_ID=your_aws_bedrock_model_id_here
AWS_REGION=your_aws_region_here  # e.g., us-west-2

# AWS credentials (if not using an IAM role or other AWS configuration methods)
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# LLM provider selection (set to AWS_BEDROCK to use the AWS plugin)
LLM_PROVIDER= "GEMINI" #"AWS_BEDROCK"
